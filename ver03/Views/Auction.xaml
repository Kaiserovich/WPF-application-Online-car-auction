            <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
            <Setter Property="Control.FontSize" Value="30" />
            <Setter Property="Control.Foreground" Value="{StaticResource grey01}" />
            <Setter Property="Control.Margin" Value="0,12,0,0" />
