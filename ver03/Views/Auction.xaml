                <TextBlock Grid.Column="1" Grid.Row="3" x:Name="tb_year" Style="{StaticResource CarInfoCont}"><!--1996--></TextBlock>
                <TextBlock Grid.Column="1" Grid.Row="4" x:Name="tb_milleage" Style="{StaticResource CarInfoCont}"><!--240 000 км--></TextBlock>
                <ScrollViewer Grid.Row="5" Grid.ColumnSpan="2" Height="100" Template="{DynamicResource ScrollViewerControlTemplate1}">
