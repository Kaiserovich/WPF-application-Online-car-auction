        <Style x:Key="ST1">
            <Setter Property="Control.FontSize" Value="30" />
            <Setter Property="Control.BorderBrush" Value="Black" />
            <Setter Property="Control.BorderThickness" Value="0"/>
            <Setter Property="Control.Foreground" Value="{StaticResource grey01}" />
            <Setter Property="Control.Margin" Value="10" />
        </Style>

        <Style x:Key="ButStyle00DEl">
            <Setter Property="Control.FontSize" Value="30" />
            <Setter Property="Control.BorderBrush" Value="Black" />
            <Setter Property="Control.BorderThickness" Value="0"/>
            <Setter Property="Control.Foreground" Value="{StaticResource grey00}" />
            <Setter Property="Control.Background" Value="{StaticResource grey01}" />
            <Setter Property="Control.Margin" Value="10" />
            <!--<Setter Property="Button.HorizontalAlignment" Value="Stretch" />-->
            <Setter Property="Button.Height" Value="50" />
        </Style>

        <Style x:Key="CarInfo">
            <Setter Property="TextBlock.HorizontalAlignment" Value="Right" />
            <Setter Property="Control.FontSize" Value="20" />
            <Setter Property="Control.Foreground" Value="{StaticResource grey01}" />
            <Setter Property="Control.Margin" Value="0,0,5,0" />
        </Style>

        <Style x:Key="CarInfoCont">
            <Setter Property="TextBlock.TextWrapping" Value="Wrap" />
            <Setter Property="Control.FontSize" Value="20" />
            <Setter Property="Control.Foreground" Value="{StaticResource grey01}" />
            <Setter Property="Control.Margin" Value="0" />
        </Style>

